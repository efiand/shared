{
  "name": "efiand-shared",
  "version": "0.2.2",
  "description": "A set of common modules (components, configurations, utilities) for any project.",
  "type": "module",
  "pre-commit": [
    "lint-format"
  ],
  "browserslist": [
    "> 0.2%",
    "last 6 version"
  ],
  "scripts": {
    "type-check": "vue-tsc --build --force",
    "dev": "vite",
    "lint-format": "npm run type-check && prettier --write . && eslint . --fix && stylelint **/*.{scss,vue} --fix",
    "lint": "npm run type-check && prettier --check . && eslint . && stylelint **/*.{scss,vue}"
  },
  "devDependencies": {
    "@fastify/pre-commit": "2.2.0",
    "eslint": "9.14.0",
    "prettier": "3.3.3",
    "stylelint": "16.10.0",
    "vite": "5.4.10",
    "vue": "3.5.12"
  },
  "dependencies": {
    "@destination/prettier-plugin-twig": "1.5.0",
    "@eslint/js": "9.14.0",
    "@eslint/markdown": "6.2.1",
    "@tsconfig/node20": "20.1.4",
    "@types/node": "22.9.0",
    "@vue/eslint-config-typescript": "14.1.3",
    "@vue/tsconfig": "0.5.1",
    "@vueform/multiselect": "2.6.10",
    "autoprefixer": "10.4.20",
    "dayjs": "1.11.13",
    "eslint-config-prettier": "9.1.0",
    "eslint-gitignore": "0.1.0",
    "eslint-plugin-perfectionist": "3.9.1",
    "eslint-plugin-vue": "9.30.0",
    "globals": "15.12.0",
    "maska": "3.0.3",
    "nanoid": "5.0.8",
    "postcss-html": "1.7.0",
    "postcss-scss": "4.0.9",
    "postcss-sort-media-queries": "5.2.0",
    "postcss-url": "10.1.3",
    "prettier-plugin-svelte": "3.2.7",
    "sass": "1.80.6",
    "stylelint-config-standard": "36.0.1",
    "stylelint-order": "6.0.4",
    "twig": "1.17.1",
    "typescript": "5.6.3",
    "typescript-eslint": "8.13.0",
    "vue-tsc": "2.1.10"
  }
}
